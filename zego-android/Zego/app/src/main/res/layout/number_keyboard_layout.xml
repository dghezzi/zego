<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:weightSum="3"
        android:layout_height="0dp">

        <it.sharethecity.mobile.letzgo.customviews.RegularRelewayTextView
            android:id="@+id/one_text_view"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:text="1"
            android:textColor="@color/gray_keyboard_button"
            android:gravity="center"
            android:textSize="@dimen/keyboard_number_size"/>
        <it.sharethecity.mobile.letzgo.customviews.RegularRelewayTextView
            android:id="@+id/two_text_view"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:text="2"
            android:textColor="@color/gray_keyboard_button"
            android:gravity="center"
            android:textSize="@dimen/keyboard_number_size"/>

        <it.sharethecity.mobile.letzgo.customviews.RegularRelewayTextView
            android:id="@+id/three_text_view"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:text="3"
            android:textColor="@color/gray_keyboard_button"
            android:gravity="center"
            android:textSize="@dimen/keyboard_number_size"/>

    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:weightSum="3"
        android:layout_height="0dp">

        <it.sharethecity.mobile.letzgo.customviews.RegularRelewayTextView
            android:id="@+id/four_text_view"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:text="4"
            android:textColor="@color/gray_keyboard_button"
            android:gravity="center"
            android:textSize="@dimen/keyboard_number_size"/>
        <it.sharethecity.mobile.letzgo.customviews.RegularRelewayTextView
            android:id="@+id/five_text_view"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:text="5"
            android:textColor="@color/gray_keyboard_button"
            android:gravity="center"
            android:textSize="@dimen/keyboard_number_size"/>

        <it.sharethecity.mobile.letzgo.customviews.RegularRelewayTextView
            android:id="@+id/siz_text_view"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:text="6"
            android:textColor="@color/gray_keyboard_button"
            android:gravity="center"
            android:textSize="@dimen/keyboard_number_size"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:weightSum="3"
        android:layout_height="0dp">

        <it.sharethecity.mobile.letzgo.customviews.RegularRelewayTextView
            android:id="@+id/seven_text_view"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:text="7"
            android:textColor="@color/gray_keyboard_button"
            android:gravity="center"
            android:textSize="@dimen/keyboard_number_size"/>
        <it.sharethecity.mobile.letzgo.customviews.RegularRelewayTextView
            android:id="@+id/eight_text_view"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:text="8"
            android:textColor="@color/gray_keyboard_button"
            android:gravity="center"
            android:textSize="@dimen/keyboard_number_size"/>

        <it.sharethecity.mobile.letzgo.customviews.RegularRelewayTextView
            android:id="@+id/nine_text_view"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:text="9"
            android:textColor="@color/gray_keyboard_button"
            android:gravity="center"
            android:textSize="@dimen/keyboard_number_size"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:weightSum="3"
        android:layout_height="0dp">

        <TextView
            android:id="@+id/nothing_text_view"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:text=""
            android:gravity="center"
            android:textSize="@dimen/keyboard_number_size"/>

        <it.sharethecity.mobile.letzgo.customviews.RegularRelewayTextView
            android:id="@+id/zero_text_view"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:text="0"
            android:textColor="@color/gray_keyboard_button"
            android:gravity="center"
            android:textSize="@dimen/keyboard_number_size"/>

        <RelativeLayout
            android:id="@+id/cancel_button"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="match_parent">
            <ImageView
                android:layout_centerInParent="true"
                android:layout_width="@dimen/cancel_img_keyboar_dimen"
                android:layout_height="@dimen/cancel_img_keyboar_dimen"
                android:src="@drawable/canc_icon"
                android:scaleType="fitCenter"
                />
        </RelativeLayout>



    </LinearLayout>

</merge>
